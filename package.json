  "scripts": {
    "build": "node --max_old_space_size=4096 ./node_modules/gulp/bin/gulp.js build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register starter/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.7",
    "@nestjs/core": "^5.3.7",
    "@nestjs/graphql": "^5.3.1",
    "@nestjs/typeorm": "^5.2.1",
    "apollo-server-express": "^2.4.2",
    "@notadd/addon-sms": "^2.4.2",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "graphql-type-json": "^0.2.1",
    "moment": "^2.24.0",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "ts-node": "^8.0.2",
    "typeorm": "^0.2.15",
    "typescript": "3.2.2",
    "underscore": "^1.9.1",
    "snyk": "^1.161.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql": "^14.0.4",
    "@types/graphql-type-json": "^0.1.3",
    "@types/i18n": "^0.8.3",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "^10.12.18",
    "gulp": "^3.9.1",
    "gulp-sequence": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0",
    "lerna": "^3.8.1",
    "nodemon": "^1.18.9",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0"
  },
  "snyk": true
}
