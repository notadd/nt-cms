{
  "name": "nt_cms",
  "version": "1.0.0",
  "description": "nt_cms",
  "author": "yk",
  "license": "MIT",
  "scripts": {
    "build": "node --max_old_space_size=4096 ./node_modules/gulp/bin/gulp.js build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register starter/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@nestjs/common": "5.7.2",
    "@nestjs/core": "5.7.2",
    "@nestjs/graphql": "5.5.3",
    "@nestjs/typeorm": "5.3.0",
    "apollo-server-express": "2.4.2",
    "@notadd/addon-sms": "2.4.2",
    "graphql": "14.1.1",
    "graphql-tools": "4.0.4",
    "graphql-type-json": "0.2.1",
    "moment": "2.24.0",
    "pg": "7.8.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.5.2",
    "ts-node": "8.0.2",
    "typeorm": "0.2.15",
    "typescript": "3.2.2",
    "underscore": "1.9.1",
    "snyk": "1.163.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/graphql": "14.2.0",
    "@types/graphql-type-json": "0.1.3",
    "@types/i18n": "0.8.5",
    "@types/jsonwebtoken": "8.3.2",
    "@types/node": "11.9.4",
    "gulp": "3.9.1",
    "gulp-sequence": "1.0.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-tslint": "8.1.4",
    "gulp-typescript": "5.0.1",
    "lerna": "3.13.4",
    "nodemon": "1.19.0",
    "ts-node": "8.0.2",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0"
  },
  "snyk": true
}
